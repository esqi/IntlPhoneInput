<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IntlPhoneInput">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="spinnerWidth" format="dimension" />
        <attr name="spinnerEndMargin" format="dimension" />
        <attr name="textColorHint" format="color" />
        <attr name="prompt" format="string" />
        <attr name="hint" format="string" />
        <attr name="hintTextAppearance" format="reference" />
        <attr name="defaultCountry" format="enum">
            <enum name="indonesia" value="0" />
        </attr>
    </declare-styleable>
</resources>
